//Codeforces
//61A - Ultra fact mathematician

#include <iostream>
#include <string>
using namespace std;

int main()
{
	string s1;
	string s2;
	cin >> s1 >> s2;

	string s3 = "";
	for(unsigned int i = 0; i < s1.size(); i++)
	{
		if(s1[i] != s2[i])
			s3 += "1";
		else
			s3 += "0";
	}

	cout << s3;
	return 0;
}
